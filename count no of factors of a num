mport java.util.Scanner;
class HelloWorld {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int i;
        int count=0;
        //int sum=0;
        for(i=1;i<=a;i++){
            if(a%i==0){
                count++;
                //sum=sum+i;
                //System.out.print(i);
               
                
                
            }
        
        }
         System.out.print(count+" ");
        
    }
}
